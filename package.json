    "bun": ">=1.0.0"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "hono": "^4.11.7",
    "openai": "^6.17.0",
    "picocolors": "^1.1.1",
    "ts-morph": "^27.0.2"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
