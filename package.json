{
  "name": "typemock-ai",
  "version": "1.0.0",
  "description": "Type is Truth. AI Mock Data Generator powered by TypeScript AST & JSDoc semantics.",
  "author": "",
  "license": "MIT",
  "type": "module",
  "bin": {
    "typemock": "./src/index.ts"
  },
  "main": "./src/index.ts",
  "files": [
